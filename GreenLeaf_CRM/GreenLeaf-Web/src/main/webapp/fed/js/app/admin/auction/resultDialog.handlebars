<div class="comm-mod5 comm-mod5-ex3">
    <div class="mod-hd">
        <h2 class="tit">物品列表</h2>
    </div>
    <div class="mod-bd">
        <div class="comm-table">
            <table class="table table-bordered table-striped">
                <thead>
                <tr>
                    <th>物品名称</th>
                    <th>物品描述</th>
                    <th>价格</th>
                    <th>最终拍卖者</th>
                    <th>积分</th>
                </tr>
                </thead>
                <tbody>
                    {{#each this}}
                        <tr>
                            <td>
                                <img src="{{getImage image}}" width="120" height="120" /><br />
                                {{name}}
                            </td>
                            <td>{{description}}</td>
                            <td>{{price}}</td>
                            <td>{{leaderName}} </td>
                            <td>{{winScore}}</td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>