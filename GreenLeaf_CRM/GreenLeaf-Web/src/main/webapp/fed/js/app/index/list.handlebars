{{#judge listData.length 0}}
    <tr>
        <td colspan="3" style="text-align: center;">
            暂无数据
        </td>
    </tr>
{{else}}
    {{#each listData}}
        {{#judge @index 0}}
            <tr class="border-none">
        {{else}}
            <tr>
        {{/judge}}
            <td style="width: 170px;"><span>{{fdate createTime}}</span></td>
            {{#judge operateType "2" }}
                <td style="width: 120px;" class="txt-c2 wnrap">+{{score}}</td>
            {{else}}
                <td style="width: 120px;" class="txt-c1 wnrap">-{{score}}</td>
            {{/judge}}
            <td>{{{urlResolve comments}}}</td>
        </tr>
    {{/each}}
{{/judge}}